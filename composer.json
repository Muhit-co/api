{
	"name": "Muhit-co/api",
	"description": "Muhit.co API",
	"keywords": ["muhit"],
	"license": "GPLv3",
	"type": "project",
	"require": {
	    "php": ">=5.5.9",
		"laravel/framework": "5.1.*",
		"guzzlehttp/guzzle": "~5.0",
		"predis/predis": "~1.0",
		"lucadegasperi/oauth2-server-laravel": "4.*",
		"facebook/php-sdk-v4": "@stable",
		"barryvdh/laravel-cors": "0.5.x@dev",
		"league/flysystem-aws-s3-v3": "~1.0",
		"jenssegers/raven": "@stable"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Models"
		],
		"psr-4": {
			"Muhit\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"npm install",
			"gulp"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"npm install",
			"gulp"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}

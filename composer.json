{
	"name": "Muhit-co/api",
	"description": "Muhit.co API",
	"keywords": ["muhit"],
	"license": "GPLv3",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"guzzlehttp/guzzle": "~5.0",
		"league/flysystem-aws-s3-v2": "~1.0",
		"predis/predis": "~1.0",
		"lucadegasperi/oauth2-server-laravel": "4.0.x@dev",
		"laravel/socialite": "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Models"
		],
		"psr-4": {
			"Muhit\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
